{
  "name": "simple-perf",
  "version": "0.0.0",
  "description": "Simplified JavaScript performance benchmarking",
  "main": "dist/index.html",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --production --config webpack.production.config.js",
    "build:watch": "webpack --watch",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-/simple-perf.git"
  },
  "keywords": [
    "simple",
    "performance",
    "jsperf",
    "benchmark"
  ],
  "author": "Jamie Hoeks <j@skeoh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-/simple-perf/issues"
  },
  "homepage": "https://github.com/j-/simple-perf#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "benchmark": "^2.1.0",
    "codemirror": "^5.14.2",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-uuid": "^1.4.7",
    "promise-worker": "^1.0.0",
    "react": "^15.0.2",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "engines": {
    "node": ">= 4"
  }
}
